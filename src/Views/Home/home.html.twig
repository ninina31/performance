{% extends "index.html.twig" %}
{% block body %}

  <div class='col-xs-12'>
    <div class='row'>
      <div class='col-xs-12'>
        <form action='/calculadora-provincia' id='calculadora'>
          <div class="form-group">
            <label for="codpostal">Código Postal</label>
            <input minlength=5 maxlength=5 type="text" class="form-control" id="codpostal" placeholder="Ingrese código postal">
          </div>
          <button type="submit" class="btn btn-default">Enviar</button>
        </form>
      </div>
    </div>
  </div>

{% endblock %}
